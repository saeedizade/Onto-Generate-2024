:MedicalDegree a owl:Class ;
               rdfs:label "Medical Degree" ;
               rdfs:comment "A degree obtained by an individual that qualifies them in the medical field." .

:Person a owl:Class ;
        rdfs:subClassOf agentrole:Agent ;
        rdfs:label "Person" ;
        rdfs:comment "An individual human being." .

:NursingAssistantDegree a owl:Class ;
                        rdfs:subClassOf :MedicalDegree ;
                        rdfs:label "Nursing Assistant Degree" ;
                        rdfs:comment "A degree that qualifies an individual to be a nursing assistant." .

:CertifiedNurseDegree a owl:Class ;
                      rdfs:subClassOf :MedicalDegree ;
                      rdfs:label "Certified Nurse Degree" ;
                      rdfs:comment "A degree that qualifies an individual to be a certified nurse." .

:hasDegree a owl:ObjectProperty ;
            rdfs:domain :Person ;
            rdfs:range :MedicalDegree ;
            rdfs:label "has degree" ;
            rdfs:comment "A property that associates a person with their obtained medical degrees." .

:PasqualeDiGennaro a :Person ;
                    :hasDegree :NursingAssistantDegree, :CertifiedNurseDegree .

:NursingAssistantDegree
    rdfs:label "Nursing Assistant Degree" ;
    rdfs:comment "A degree that certifies an individual as a nursing assistant." .

:CertifiedNurseDegree
    rdfs:label "Certified Nurse Degree" ;
    rdfs:comment "A degree that certifies an individual as a certified nurse." .

:hasMedicalDegree a owl:ObjectProperty ;
                  rdfs:domain :Person ;
                  rdfs:range :MedicalDegree ;
                  rdfs:label "has medical degree" ;
                  rdfs:comment "Associates a person with their medical degrees." .

:obtainedDegreeOn a owl:DatatypeProperty ;
                   rdfs:domain [:unionOf (:NursingAssistantDegree :CertifiedNurseDegree)] ;
                   rdfs:range xsd:date ;
                   rdfs:label "obtained degree on" ;
                   rdfs:comment "The date on which the degree was obtained." .

:PasqualeDiGennaro :hasMedicalDegree :CertifiedNurseDegree ;
                    :obtainedDegreeOn "2001-05-01"^^xsd:date .

:StudyPeriod a owl:Class ;

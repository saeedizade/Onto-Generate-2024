: a owl:Ontology ;
   rdfs:comment "An ontology for representing information about theatre festivals and productions."@en .

### Classes

:TheatreFestival a owl:Class ;
    rdfs:label "Theatre Festival"@en ;
    rdfs:comment "A festival event that includes one or more theatre productions."@en .

:TheatreProduction a owl:Class ;
    rdfs:label "Theatre Production"@en ;
    rdfs:comment "A theatrical performance or series of performances."@en .

:City a owl:Class ;
    rdfs:label "City"@en ;
    rdfs:comment "A large human settlement."@en .

:Play a owl:Class ;
    rdfs:label "Play"@en ;
    rdfs:comment "A form of literature intended for theatrical performance."@en .

:Actor a owl:Class ;
    rdfs:label "Actor"@en ;
    rdfs:comment "A person who portrays a character in a performance."@en .

### Object Properties

:tookPlaceIn a owl:ObjectProperty ;
    rdfs:label "took place in"@en ;
    rdfs:comment "The city in which a theatre festival took place."@en ;
    rdfs:domain :TheatreFestival ;
    rdfs:range :City .

:featuredProduction a owl:ObjectProperty ;
    rdfs:label "featured production"@en ;
    rdfs:comment "A theatre production that is part of a festival's program."@en ;
    rdfs:domain :TheatreFestival ;
    rdfs:range :TheatreProduction .

:hasPlay a owl:ObjectProperty ;
    rdfs:label "has play"@en ;
    rdfs:comment "The play that is performed in a theatre production."@en ;
    rdfs:domain :TheatreProduction ;
    rdfs:range :Play .

:hasActor a owl:ObjectProperty ;
    rdfs:label "has actor"@en ;
    rdfs:comment "An actor that performs in a theatre production."@en ;
    rdfs:domain :TheatreProduction ;
    rdfs:range :Actor .

### Datatype Properties

:startDate a owl:DatatypeProperty ;
    rdfs:label "start date"@en ;
    rdfs:comment "The starting date of a festival or production."@en ;
    rdfs:domain :TheatreFestival ;
    rdfs:range xsd:date .

### Individuals

:RomaLovesShakespeare a :TheatreFestival ;
    rdfs:label "Roma Loves Shakespeare"@en ;
    :tookPlaceIn dbr:Rome ;
    :startDate "2007-01-01"^^xsd:date .

:MerchantOfVenice_Pisa a :TheatreProduction ;
    rdfs:label "The Merchant of Venice (Pisa)"@en ;
    :hasPlay dbr:The_Merchant_of_Venice .

:MerchantOfVenice_Venice a :TheatreProduction ;
    rdfs:label "The Merchant of Venice (Venice)"@en ;
    :hasPlay dbr:The_Merchant_of_Venice .

:Othello a :Play ;
    rdfs:label "Othello"@en .

:MidsummerNightsDream a :Play ;
    rdfs:label "A Midsummer Night's Dream"@en .

### Restrictions

:TheatreFestival rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :featuredProduction ;
    owl:someValuesFrom :TheatreProduction
] .

:TheatreProduction rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasPlay ;
    owl:someValuesFrom :Play
] .

:TheatreProduction rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasActor ;
    owl:someValuesFrom :Actor
] .

